{
  "name": "lazy-motion",
  "main": "./lib/main",
  "version": "0.1.17",
  "description": "Rapid cursor positioning with fuzzy search",
  "keywords": [],
  "repository": "https://github.com/t9md/atom-lazy-motion",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationCommands": {
    "atom-text-editor": [
      "lazy-motion:forward",
      "lazy-motion:backward",
      "lazy-motion:forward-again",
      "lazy-motion:backward-again",
      "lazy-motion:forward-cursor-word",
      "lazy-motion:backward-cursor-word"
    ]
  },
  "dependencies": {
    "atom-config-plus": "^0.1.2",
    "fuzzaldrin": "^2.1.0",
    "underscore-plus": "^1.6.6"
  },
  "configSchema": {
    "autoLand": {
      "order": 0,
      "type": "boolean",
      "default": false,
      "description": "automatically land(confirm) if there is no other candidates"
    },
    "minimumInputLength": {
      "order": 1,
      "type": "integer",
      "minimum": 0,
      "default": 0,
      "description": "Search start only when input length exceeds this value"
    },
    "wordRegExp": {
      "order": 2,
      "type": "string",
      "default": "[@\\w-.():?]+",
      "description": "Used to build candidate List"
    },
    "showHoverIndicator": {
      "order": 3,
      "type": "boolean",
      "default": true
    },
    "historySize": {
      "order": 4,
      "type": "integer",
      "minimum": 0,
      "default": 30
    },
    "saveHistoryOnCancel": {
      "order": 5,
      "type": "boolean",
      "default": true,
      "description": "If false, canceled search won't saved to history."
    }
  }
}
