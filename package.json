{
  "name": "lazy-motion",
  "main": "./lib/main",
  "version": "0.1.17",
  "description": "Rapid cursor positioning with fuzzy search",
  "keywords": [],
  "repository": "https://github.com/t9md/atom-lazy-motion",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationCommands": {
    "atom-text-editor": [
      "lazy-motion:forward",
      "lazy-motion:backward",
      "lazy-motion:forward-again",
      "lazy-motion:backward-again",
      "lazy-motion:forward-cursor-word",
      "lazy-motion:backward-cursor-word"
    ]
  },
  "dependencies": {
    "fuzzaldrin-plus": "^0.3.1",
    "underscore-plus": "^1.6.6"
  },
  "configSchema": {
    "wordRegExp": {
      "order": 2,
      "type": "string",
      "default": "[@\\w-.():?]+",
      "description": "Used to build candidate List"
    },
    "showHoverIndicator": {
      "order": 3,
      "type": "boolean",
      "default": true
    },
    "historySize": {
      "order": 4,
      "type": "integer",
      "minimum": 0,
      "default": 30
    },
    "saveHistoryOnCancel": {
      "order": 5,
      "type": "boolean",
      "default": true,
      "description": "If false, canceled search won't saved to history."
    }
  }
}
